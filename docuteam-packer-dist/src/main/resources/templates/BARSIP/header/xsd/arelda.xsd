                <xs:sequence>
                    <xs:element name="ablieferungsnummer" type="ablieferungsnummer"/>
                    <xs:element name="angebotsnummer" minOccurs="0" type="angebotsnummer"/>
                    <xs:element name="referenzBewertungsentscheid" minOccurs="0"
