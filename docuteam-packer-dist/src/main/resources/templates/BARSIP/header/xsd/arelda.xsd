            <xs:element name="existenzzeitraum" minOccurs="0" type="historischerZeitraum">
				        <xs:annotation>
					          <xs:documentation>Zeitraum der Existenz des Aktenbildners.</xs:documentation>
				        </xs:annotation>
			      </xs:element>
            <xs:element name="geschichteAktenbildner" minOccurs="0" type="geschichteAktenbildner"/>
