                    <xs:element name="schutzfrist" minOccurs="0" type="schutzfristAblieferung"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="ablieferungGeverAIP">
